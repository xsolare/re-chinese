{
  "name": "re-chinese",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ssl": "node server.js",
    "build": "next build",
    "build:stats": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "start:ssl": "NODE_ENV=production node server.js",
    "sitemap": "next-sitemap",
    "postbuild": "next-sitemap",
    "export": "BROWSERSLIST_IGNORE_OLD_DATA=true next export",
    "lint": "pnpm lint:ts && pnpm lint:scss",
    "lint:ts": "eslint \"./**/*.{ts,tsx}\"",
    "lint:scss": "stylelint \"./**/*.scss\" --allow-empty-input",
    "lint:fix": "eslint --fix --ext \"./**/*.{ts,tsx}\" && stylelint --fix \"./**/*.scss\" --allow-empty-input",
    "lint:staged": "lint-staged --debug",
    "format": "prettier -w \"./**/*.{ts,tsx,json,scss,md}\"",
    "prepare": "husky install .husky",
    "clean": "rm -rf .next && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@popperjs/core": "^2.11.7",
    "@tanstack/react-virtual": "3.0.0-beta.54",
    "@vercel/analytics": "^0.1.11",
    "@xsolare/ui": "^1.1.6",
    "antd": "^5.4.0",
    "axios": "^1.3.4",
    "browserslist": "^4.21.5",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "dotenv-load": "^2.0.1",
    "html-react-parser": "^3.0.15",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "next": "13.2.4",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "next-images": "^1.8.4",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.0.6",
    "nookies": "^2.5.2",
    "rc-virtual-list": "^3.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-popper": "^2.3.0",
    "react-virtual": "^2.10.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@emotion/eslint-plugin": "^11.10.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@next/env": "*",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@xsolare/eslint-plugin": "^1.0.7",
    "autoprefixer": "^10.4.14",
    "commit-message-validator": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.3.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^11.0.0",
    "typescript": "5.0.3",
    "webpack": "^5.77.0"
  },
  "packageManager": "pnpm@7.18.2",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "src/**/*.+(scss)": [
      "stylelint --fix --allow-empty-input"
    ],
    "src/**/*.+(js|jsx|ts|tsx|json|scss|md)": [
      "prettier --write",
      "prettier -c"
    ]
  },
  "config": {
    "commit-message-validator": {
      "pattern": "(feat|fix|fix-r|refactor)?:",
      "errorMessage": "Invalid commit message"
    }
  },
  "peerDependencies": {
    "@next/env": "*",
    "webpack": ">=5.9.0 <6.0.0"
  }
}
